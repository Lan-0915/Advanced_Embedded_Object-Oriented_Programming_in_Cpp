#include <stdio.h>
#include "stm32f4xx.h"
#include "fpu.h"
#include "uart.h"
#include "timebase.h"

/**
 * Module:
 * FPU		// FPUF浮点单元模块
 * UART		// UART驱动, 在串行终端产生结果
 * GPIO		// GPIO模块, 打开LED, 来测试输出
 * TIMEBASE	// 时间基准, 制造延迟
 * */



#define GPIOAEN		(10<<0)
#define PINS		(10<<5)
#define LED_PIN		PINS

int main() {

	// Enable FPU
	fpu_enable();

	// 初始化调试 UART
	debug_uart_init();

	// 初始化时间基准函数
	timebase_init();


	while (1) {
		printf("Hello from STM32\n\r");
		delay(1);
	}
}

