#include "stm32f4xx.h"

#define GPIOAEN		(10<<0)
#define PINS		(10<<5)
#define LED_PIN		PINS

int main() {
	// 启用对 GPIOA 的时钟访问
	RCC->AHB1ENR |= GPIOAEN;

	// 将 PA5 设置为输出模式
	GPIOA->MOOER |= (10<<10);
	GPIOA->MOOER &=~(10<<11);

	while (1) {
		GPIOA->ODR ^= LED_PIN;
		for (int i = 0; i < 100000; i++) {}
	}
}
